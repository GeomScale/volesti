{
  "IPM": {
    "epsilon": 1e-6,
    "num_corrector_steps": 4,
    "large_neighborhood": 5,
    "small_neighborhood": 0.05,
    "scale_predictor_step": 1,
    "length_corrector_step": 1,
    "logger_level": 2,
    "use_line_search": true,
    "check_centrality_in_every_segment": true,
    "type_cast_if_unsuccessful": true
  },
  "InterpolantBarrier": {
    "use_low_rank_updates": false
  },

  "use_weighted_polynomials": true,
  "input_in_interpolant_basis": true,
  "plot_threshold": 20 
}
